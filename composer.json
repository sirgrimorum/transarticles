    "keywords": [
        "sirgrimorum",
        "TransArticles",
        "Localizarion"
    ],
    "homepage": "http://desarrollo.grimorum.com/andres/transarticles",
    "license": "MIT",
    "authors": [
        {
            "name": "Sirgrimorum",
            "email": "andres.espinosa@grimorum.com"
        }
    ],
    "require": {
